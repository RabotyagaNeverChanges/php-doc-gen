    "type": "project",
    "autoload": {
        "psr-4": {
            "PhpDocGen\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Kharlamov Vladislav",
            "email": "kharlamov.workspace@gmail.com"
        }
    ],
    "config": {
